{
  "name": "unofficial-ravensburger-playhub-api",
  "version": "0.3.0",
  "description": "Unofficial Ravensburger PlayHub API client for Disney Lorcana events, stores, standings, cards, and leaderboards",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/index.*",
    "dist/types.*",
    "dist/http.*",
    "dist/api.*",
    "dist/search.*",
    "dist/leaderboard.*",
    "dist/format.*",
    "dist/geocode.*",
    "dist/ttlCache.*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test:integration": "npm run build && node --test dist/test/integration.test.js",
    "test": "npm run test:integration",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "lorcana",
    "ravensburger",
    "playhub",
    "api",
    "disney-lorcana",
    "events",
    "tournaments"
  ],
  "license": "Unlicense",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "typescript": "^5.9.3"
  }
}
